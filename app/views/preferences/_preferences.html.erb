<p>
<% case @preferences.kind_of %>
	<% when 1 %>
		200: A, 150 B, 100 C, 50 D
	<% when 2 %>
		200: D, 150 A, 100 B, 50 C
	<% when 3 %>
		200: C, 150 D, 100 A, 50 B
	<% when 4 %>
		200: B, 150 C, 100 D, 50 A
	<% when 5 %>
		200: A, 150 C, 100 B, 50 D
	<% when 6 %>
		200: D, 150 B, 100 C, 50 A
	<% else %>
		FAIL
<% end %>
</p>

<div class="row">
  <h1>Round Preferences</h1>
</div>

<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Project Name</th>
      <th>Project A</th>
      <th>Project B</th>
      <th>Project C</th>
      <th>Project D</th>
    </tr>
  </thead>
  <tbody>
    <th>Project Payout</th>
    <th>200 Credits</th>
    <th>150 Credits</th>
    <th>100 Credits</th>
    <th>50 Credits</th>
  </tbody>
</table>