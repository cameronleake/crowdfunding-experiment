<h1>Round Summary</h1>
<p>&nbsp;</p>
<div class="row">
  <div class="span2">
    <h3>Project Name</h3>
  </div>
  
  <div class="span4">
    <h3>Funding Status</h3>
  </div>
  
  <div class="span6">
    &nbsp;
  </div>
</div>
<p>&nbsp;</p>

<% round.projects.each do |p| %>
	<%= render :partial => 'projects/stats', :locals => { :project => p }%>
<% end %>

<table class="table table-striped table-bordered">
<thead><tr><td>User</td><td>Payout (for this round)</td></tr></thead>
<% round.users.each do |u| %>
 <tr>
 	<td><%= u.name %></td>
 	<td><%= u.preferences.where(:round_id => round.id).first.round_payout %></td>
 </tr>
<% end %>
</table>