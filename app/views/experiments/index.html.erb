<h1>Listing Experiments</h1>

<table class="table-striped">
	<thead>
  <tr>
    <td>Experiment Id</td>
    <td>Start Time</td>
    <td>Return Credits</td>
    <td>Actions</td>
    <td></td>
  </tr>
	</thead>

<% @experiments.each do |experiment| %>
  <tr>
  	<td><%= experiment.id %></td>
  	<td><%=l experiment.start_time, :format => :default %></td>
  	<td><%= experiment.return_credits %></td>
    <td><%= link_to 'Show', dashboard_path(experiment) %></td>
    <td><%= link_to 'Destroy', experiment, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Experiment', new_experiment_path %>
